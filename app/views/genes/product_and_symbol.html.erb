<h1>Genes with product and symbol both specified <small>Threshold greater than <%= @limit %> sequences</small></h1>

<table id="symbols-table" class="table">
  <thead>
    <tr>
      <th>Product</th>
      <th>Symbol</th>
      <th>Sequences</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  <% @genes.each do |k,v| %>
      <tr>
        <td><%= k[1] %></td>
        <td><%= k[0] %></td>
        <td><%= v %></td>
        <td></td>
      </tr>
  <% end %>
  </tbody>
</table>

<script>
var table = $('#symbols-table').DataTable({pageLength: 100});
</script>
